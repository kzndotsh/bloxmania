{
  "name": "bloxmania-shopify-theme",
  "version": "1.0.0",
  "description": "BloxMania - Digital Goods Marketplace Shopify Theme",
  "main": "index.js",
  "scripts": {
    "dev": "shopify theme dev",
    "push": "shopify theme push",
    "pull": "shopify theme pull",
    "check": "shopify theme check",
    "check:fix": "shopify theme check --auto-correct",
    "package": "./deploy.sh",
    "package:cli": "./deploy.sh --cli",
    "init": "shopify theme init",
    "list": "shopify theme list",
    "info": "shopify theme info",
    "publish": "shopify theme publish",
    "share": "shopify theme share",
    "open": "shopify theme open",
    "rename": "shopify theme rename",
    "delete": "shopify theme delete",
    "console": "shopify theme console",
    "profile": "shopify theme profile",
    "test": "shopify theme check --output=json > theme-check-report.json",
    "lint": "shopify theme check",
    "clean": "rm -rf node_modules package-lock.json",
    "setup": "npm install"
  },
  "keywords": [
    "shopify",
    "theme",
    "ecommerce",
    "gaming",
    "digital-goods",
    "roblox",
    "fortnite"
  ],
  "author": "BloxMania Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/bloxmania-shopify-theme.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/bloxmania-shopify-theme/issues"
  },
  "homepage": "https://github.com/your-username/bloxmania-shopify-theme#readme",
  "devDependencies": {
    "@shopify/cli": "^3.0.0",
    "@shopify/theme": "^3.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "shopify": {
    "theme": {
      "name": "BloxMania Theme",
      "version": "1.0.0",
      "description": "Digital goods marketplace theme for gaming items"
    }
  },
  "config": {
    "shopify": {
      "store": "tnqzbi-xv.myshopify.com",
      "theme_id": "auto"
    }
  }
}